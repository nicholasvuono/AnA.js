{
  "name": "ana.js",
  "version": "1.0.0",
  "description": "Full-stack Authentication and Access Management Application for the MERN Stack",
  "main": "./",
  "scripts": {
    "install:client": "npm install --prefix client",
    "start:client": "npm start --prefix client",
    "start:server": "node ./server/server.js",
    "start:dev-server": "nodemon ./server/server.js",
    "start:client-server": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholasvuono/AnA.js.git"
  },
  "author": "Nicholas Vuono",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicholasvuono/AnA.js/issues"
  },
  "homepage": "https://github.com/nicholasvuono/AnA.js#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "bootstrap": "^4.6.2",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
